{
  "name": "lunchvue",
  "version": "0.1.1",
  "description": "Provides lunch info about kr",
  "main": "app/index.js",
  "repository": "https://github.com/Kurosnape/LunchVue",
  "author": "Kurosnape <shuvi.dev.b701@gmail.com> (https://kurosnape.github.io)",
  "scripts": {
    "start": "node ./app/index.js",
    "dev": "nodemon -e ts -w ./dev-app -x yarn run dev:watch",
    "dev:watch": "ts-node dev-app/index.ts",
    "sass": "node-sass --output-style compressed assets/src/sass/app.sass -o assets/dist",
    "sass:watch": "nodemon -e sass -w ./assets/src/sass -x yarn run sass",
    "ts": "tsc -p assets/src/ts/tsconfig.json"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "pug": "^2.0.3",
    "request": "^2.85.0"
  },
  "devDependencies": {
    "@types/compression": "^0.0.36",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.6.2",
    "@types/request": "^2.47.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "4.1.0",
    "compass-mixins": "^0.12.10",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.3",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1"
  },
  "license": "MIT",
  "private": true
}
